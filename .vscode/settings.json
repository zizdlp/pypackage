{
    "editor.snippetSuggestions": "top",
    "editor.minimap.renderCharacters": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": true,
    "editor.wordWrap": "on",
    "C_Cpp.updateChannel": "Insiders",
    "http.proxySupport": "off",
    "editor.tabSize": 2,
    "team.showWelcomeMessage": false,
    "editor.rulers": [
        120
    ],
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.cStandard": "c11",
    "editor.minimap.maxColumn": 120,
    "C_Cpp.clang_format_sortIncludes": false,
    "[cpp]": {
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "editor.formatOnSave": true,
    "C_Cpp.clang_format_style": "{ BasedOnStyle: google, ColumnLimit: 120, PointerAlignment: Left, IndentWidth: 2, AllowAllParametersOfDeclarationOnNextLine: true,TabWidth: 2, ContinuationIndentWidth: 4, AccessModifierOffset: -1, Standard:  Cpp11, BinPackParameters: false, BinPackArguments: false}",
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#ff4081"
    },
    "editor.wordBasedCompletion": {
        "inStrings": true,
        "inComment": false,
        "inCode": true
    },
